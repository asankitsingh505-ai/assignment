def factorial(a):
    if a<=1:
        return 1
    else:
        return a * factorial(a - 1)
a=int(input("enter a number:"))
result = factorial(a)
print("factorial of",a,"is :",result)


a = int(input("input a number: "))
import math
result1= math.sqrt(a)
result2 = math.log(a)
result3 = math.sin(a)
print("square root :",result1)
print("logarithm  :",result2)
print("sine :",result3)

